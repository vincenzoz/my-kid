{
  "buildCommand": "npm run build:all",
  "outputDirectory": "build",
  "rewrites": [
    { "source": "/api/(.*)", "destination": "/api/$1" },
    {
      "source": "/((?!api/|.*\\.).*)",
      "destination": "/browser/index.html"
    },
    {
      "source": "/:file((?!api/).+\\..+)",
      "destination": "/browser/:file"
    }

  ]
}
